{
  "name": "stopcitingai",
  "compatibility_date": "2025-12-16",
  "build": {
    "command": "pip install jinja2 && python build.py"
  },
  "assets": {
    "directory": "./public"
  },
  "observability": {
    "enabled": false,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    }
  }
}